@model airbornefrs.Models.SupportCorporateAdvertising

@{
    ViewBag.Title = "Airborne FRS: Corporte Advertising";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <link href="~/Content/intlTelInput.css" rel="stylesheet" />
    <link href="~/external/bootstrap-select/bootstrap-select.css" rel="stylesheet">
    <link href="~/external/mCustomScrollbar/jquery.mCustomScrollbar.css" rel="stylesheet" />

        <!-- Section show current page and breadcrumb -->
        <section class="page-indecator">
            <div class="container">
                <h2 class="heading">Corporate Advertising</h2>
                <!-- Breadcrumb pattern -->
                <ol class="breadcrumb breadcrumb-pages">
                    <li><a href="~/Home/Index">Home</a></li>
                    <li class="active">Corporate Advertising</li>
                </ol>
                <!-- Default one color devider -->
                <div class="devider"></div>
            </div>
        </section>
        <!-- end section show current page and breadcrumb -->
        <div class="inner-page-template extra clearfix" ng-controller="CorporateAdvertisingCtrl">
            <div class=" container">
                <h2>Corporate Advertising</h2>
    <div class="row">
        <div class="col-xs-12 col-sm-6 form-text">
            <h3>Corporate Advertising</h3>
            <p class="p-space">We offer branding and marketing opportunities through our network of Resellers and software products. By strategically partnering with selected advertisers, we increase the value to our customer and make are products more affordable.</p>
            <p class="p-space">To learn more about how our branding programs work, please complete the form. Once you submit the form on this page, you will receive an email message with the support ticket identification. Our Ground Crew will then contact you via the email support ticket. Thank you.</p>
        </div>
        <div class="col-xs-12 col-sm-6 support-form contact-form">
            <h3>Corporate Advertising</h3>
            <div class="fmessage" style="color:#0d3a60;"></div></br>
            <form method="post" class="contact" role="form" ng-focus="true" ng-submit="submitForm()" name="SupportCorporateAdvertising" id="SupportCorporateAdvertising">
                <div class="form-group has-feedback">
                    <label>*Name</label>
                    <input type="text" class="contact__field" placeholder="" ng-model="data.CorporateAdvertisingData.Name" name="Name">
                </div>
                <div class="form-group has-feedback">
                    <label>*Email Address</label>
                    <input type="text" class="contact__field" placeholder="" ng-model="data.CorporateAdvertisingData.Email" name="Email">
                </div>
                <div class="form-group has-feedback">
                    <label>Company Name</label>
                    <input type="text" class="contact__field" placeholder="" ng-model="data.CorporateAdvertisingData.CompanyName" name="CompanyName">
                </div>
                <div class="form-group has-feedback">
                    <label>*Telephone number</label>
                    <input type="text" class="contact__field" placeholder="" ng-model="data.CorporateAdvertisingData.Telephone" name="Telephonenumber">

                    <div class="radio radio--dark">
                        <input type="radio" value="Landline" name="Teletype" ng-model="data.CorporateAdvertisingData.Teletype" id="Landline" data-fv-field="Landline">
                        Landline
                    </div>
                    <div class="radio radio--dark">
                        <input type="radio" value="Mobile" name="Teletype" ng-model="data.CorporateAdvertisingData.Teletype" id="Mobile" data-fv-field="Mobile">
                        Mobile
                    </div>
                </div>
                <div class="form-group has-feedback">
                    <label>*Primary Type Of Buisness</label>
                    <select class="select-box" name="PrimaryTypeOfBuisness" ng-model="data.CorporateAdvertisingData.BuisnessType">
                        <option value="" selected="selected">Select One</option>
                        <option value="Aviation Speciality">Aviation Speciality </option>
                        <option value="Customer Product">Customer Product</option>
                        <option value="Dealer Representative">Dealer Representative</option>
                        <option value="eCommerce/catolog Store">eCommerce/catolog Store</option>
                        <option value="Educational Institution">Educational Institution</option>
                        <option value="FBO">FBO </option>
                        <option value="Retail Store">Retail Store</option>
                        <option value="Retail Store(at airport)">Retail Store(at airport)</option>
                        <option value="Training center">Training center</option>
                        <option value="Travel Agency">Travel Agency</option>
                        <option value="US Military">US Military</option>
                        <option value="Others">Others</option>
                    </select>
                </div>
                <div class="form-group has-feedback">
                    <label>*Location</label>
                    <div class="radio radio--dark">
                        <input type="radio" value="USA" name="Location" ng-model="data.CorporateAdvertisingData.Location" id="USA" data-fv-field="USA">
                        USA
                    </div>
                    <div class="radio radio--dark">
                        <input type="radio" value="International" name="Location" ng-model="data.CorporateAdvertisingData.Location" id="International" data-fv-field="International">
                        International
                    </div>
                </div>

                <div class="form-group has-feedback">
                    <label>*How can we help?</label>
                    <textarea class="contact__field" ng-model="data.CorporateAdvertisingData.AdditionalInfo" name="AdditionalInfo"></textarea>
                </div>

                <div class="form-group has-feedback">
                    <label>Human Test </label>
                    <div id="captchaContainer">
                        <div class="g-recaptcha" data-sitekey="6Lfppw8TAAAAAPyLD_-5SAbI_6b3z_l48nFZVJiz"></div>
                    </div>
                </div>
                <div class="form-group has-feedback">
                    <button id="submit_btn" type="submit" name="submitButton" class="btn btn-warning btn--decorated">Submit</button>
                </div>
            </form>
            <div id="dvAfterSubmit"></div>
        </div>
    </div>
</div>
            <!-- end container -->
        </div>
        <!-- Modal -->
        <div class="modal fade--zoom" aria-labelledby="fastfacts" tabindex="-1" role="dialog" id="fastfacts">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times"></i></button>
                        <h4 class="modal-title" id="fastfacts">FastFacts</h4>
                    </div>
                    <div class="modal-body">
                        <ul class="list">
                            <li> Airborne Flight Reporting System: for all Air &amp; Ground Crews, Passengers, Family &amp; Associates!</li>
                            <li>Affordable Flight Tracking &amp; Management: great for General Aviation flight either VFR &amp; IFR!</li>
                            <li>Airborne FRS for Vertical Lift: Reports on the unique Flight Characteristics of VTOLs!</li>
                            <li> Lightweight for the Drones: Provides enhanced Flight Monitoring for Drones!</li>
                            <li>Makes Unpredictable Flight Safer: If it Takes-off, is should have Airborne!</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="top-scroll"><i class="fa fa-angle-up"></i></div>
@section BottomScript{
    <script>
        var objCorporateAdvertising = @Html.Raw(Model.GetJsonData());
    </script>
    <script src="~/assets/js/controllers/supportCorporateAdvertising.js"></script>
}
