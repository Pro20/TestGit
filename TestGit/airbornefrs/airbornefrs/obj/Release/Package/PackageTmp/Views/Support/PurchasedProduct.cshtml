@model airbornefrs.Models.PurchasedProductModel
@{
    ViewBag.Title = "Airborne FRS: PurchasedProduct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



   <link href="~/external/bootstrap-select/bootstrap-select.css" rel="stylesheet">
   <link href="~/external/mCustomScrollbar/jquery.mCustomScrollbar.css" rel="stylesheet" />
   

    
<!-- Section show current page and breadcrumb -->
        <section class="page-indecator">
            <div class="container">
                <h2 class="heading">Purchased Product</h2>

                <!-- Breadcrumb pattern -->
                <ol class="breadcrumb breadcrumb-pages">
                    <li><a href="~/Home/Index">Home</a></li>
                    <li class="active">Purchased Product</li>
                </ol>
                <!-- Default one color devider -->
                <div class="devider"></div>
            </div>
        </section>
        <!-- end section show current page and breadcrumb -->
        <div class="inner-page-template extra clearfix">
            <div class="container">
                <h2>Purchased Product</h2>
                <div class="row">
                    <div class="col-xs-12 col-sm-6 form-text">
                        <h3>For Existing Airborne Owners</h3>
                        <p class="p-space">Use this form if you are an existing Airborne product owner or if you are a fleet manager or operate more than one Airborne product. Mobile application users please use the Mobile Application Support form.</p>

                        <p>Once you submit the form on this page, you will receive an email message with the support ticket identification. Our Ground Crew will then contact you via the email support ticket. Thank you.</p>
                    </div>
                    <div class="col-xs-12 col-sm-6 support-form" ng-controller="purchasedProductCtrl">
                        <h3>Purchased Product Form</h3>
                        <form class="contact" method="post" role="form" ng-focus="true" ng-submit="submitForm()" name="purchasedproductForm" id="purchasedproductForm">
                            <div class="form-group">
                                <label>*Name</label>
                                <input type="text" class="contact__field" placeholder="" id="Name" name="Name" ng-model="data.purchasedproductData.Name">
                            </div>
                            <div class="form-group">
                                <label>*Email Address</label>
                                <input type="text" class="contact__field" placeholder="" id="Email" name="Email" ng-model="data.purchasedproductData.Email">
                            </div>
                            <div class="form-group">
                                <label>Company Name</label>
                                <input type="text" class="contact__field" placeholder="" id="Companyname" name="Companyname" ng-model="data.purchasedproductData.Companyname">
                            </div>
                            <div class="form-group">
                                <label>*Product</label>
                                <select class="select-box" id="select-box" name="product" tabindex="0" ng-model="data.purchasedproductData.product">
                                    <option value="">Select Product...</option>
                                    <option value="Airborne C100">Airborne C100 </option>
                                    <option value="Airborne C100">Airborne C100</option>
                                    <option value="Airborne S100">Airborne S100</option>
                                    <option value="Airborne S200">Airborne S200</option>
                                    <option value="Airborne D500">Airborne D500</option>
                                    <option value="VISability™ Portal">VISability™ Portal </option>
                                    <option value="Airborne Flight Sense™ Base Sensor System">Airborne Flight Sense™ Base Sensor System</option>
                                    <option value="Airborne Flight Sense™ Sensors">Airborne Flight Sense™ Sensors</option>
                                    <option value="Airborne FRS Training Program">Airborne FRS Training Program</option>
                                    <option value="ADS - B Interoperability Demonstration Kit">ADS - B Interoperability Demonstration Kit</option>
                                    <option value="ADS - B Interoperability Radio">ADS - B Interoperability Radio</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label>* Product IMEI number</label>
                                <input type="text" class="contact__field" name="IMEI" id="IMEI" ng-model="data.purchasedproductData.IMEI">
                            </div>
                            <div class="form-group">
                                <label>*Location</label>
                                <div class="radio radio--dark">
                                    <input type="radio" value="USA" name="Location" id="usa" ng-model="data.purchasedproductData.Location">
                                    <label for="usa">USA</label>
                                </div>
                                <div class="radio radio--dark">
                                    <input type="radio" value="International" name="Location" id="international" ng-model="data.purchasedproductData.Location">
                                    <label for="international">International   </label>
                                </div>
                            </div>
                            <div class="form-group">
                                <label>*How can we help?</label>
                                <textarea class="contact__field" name="Howcanwehelp" ng-model="data.purchasedproductData.Howcanwehelp"></textarea>
                            </div>
                            <div class="form-group clearfix">
                                <label>Human Test </label>
                                <div id="captchaContainer">
                                    <div class="g-recaptcha" data-sitekey="6Lfppw8TAAAAAPyLD_-5SAbI_6b3z_l48nFZVJiz"></div>
                                </div>
                            </div>
                            @*<div class="form-group clearfix">
                                <label>Human Test </label>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-6 form-group" id="captchaContainer">
                                        <div class="g-recaptcha" data-sitekey="6Lfppw8TAAAAAPyLD_-5SAbI_6b3z_l48nFZVJiz"></div>
                                    </div>
                                </div>
                            </div>*@
                            <div class="form-group">
                                <button id="submit_btn" type="submit" name="submitButton" class="btn btn--decorated btn-warning btn-submit">Submit</button>
                            </div>
                        </form>
                        <div id="dvAfterSubmit"></div>
                    </div>
                </div>
            </div><!-- end container -->
        </div>

        
        <!-- Modal -->
        <div class="modal fade--zoom" aria-labelledby="fastfacts" tabindex="-1" role="dialog" id="fastfacts">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times"></i></button>
                        <h4 class="modal-title" id="fastfacts">FastFacts</h4>
                    </div>
                    <div class="modal-body">
                        <ul class="list">
                            <li> Airborne Flight Reporting System: for all Air &amp; Ground Crews, Passengers, Family &amp; Associates!</li>
                            <li>Affordable Flight Tracking &amp; Management: great for General Aviation flight either VFR &amp; IFR!</li>
                            <li>Airborne FRS for Vertical Lift: Reports on the unique Flight Characteristics of VTOLs!</li>
                            <li> Lightweight for the Drones: Provides enhanced Flight Monitoring for Drones!</li>
                            <li>Makes Unpredictable Flight Safer: If it Takes-off, is should have Airborne!</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="top-scroll"><i class="fa fa-angle-up"></i></div>
@section BottomScript{
    <script>
        var objPurchasedProduct = @Html.Raw(Model.GetJsonData());
    </script>
 
<script src="~/assets/js/controllers/purchasedProductCtrl.js"></script>
}



